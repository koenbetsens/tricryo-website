{
  "name": "tricryo-website",
  "version": "0.0.4",
  "description": "React website of tricryo",
  "main": "dist/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server",
    "mock-api": "json-server --watch __mocks__/db.json --port 9004",
    "prebuild": "yarn test && rimraf dist",
    "build": "cross-env NODE_ENV=production webpack --progress",
    "test": "cross-env NODE_ENV=test jest --c ./jest.json --coverage",
    "precommit": "yarn test",
    "commit": "git-cz",
    "prestart": "yarn build",
    "start": "serve -s dist -l 8080",
    "open-cov": "open coverage/lcov-report/index.html",
    "prerelease": "git fetch --tags",
    "release": "standard-version",
    "generate": "node ./internals/generate",
    "storybook": "start-storybook -p 9002 -c ./src/storybook",
    "storybook:dist": "build-storybook -c ./src/storybook -o docs",
    "preopen-storybook": "yarn storybook:dist",
    "open-storybook": "open docs/index.html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Cloudoki/tricryo-website.git"
  },
  "keywords": [
    "tricryo",
    "react"
  ],
  "contributors": [
    "koenbetsens <koen@tricryo.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Cloudoki/tricryo-website/issues"
  },
  "homepage": "https://github.com/Cloudoki/tricryo-website.git#readme",
  "dependencies": {
    "history": "4.7.2",
    "immutability-helper": "2.9.0",
    "js-cookie": "2.2.0",
    "prop-types": "15.6.0",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-ga": "^2.7.0",
    "react-intl": "2.4.0",
    "react-markdown": "^4.3.0",
    "react-redux": "5.1.1",
    "react-router": "4.3.1",
    "react-router-redux": "5.0.0-alpha.9",
    "redux": "4.0.1",
    "redux-saga": "0.16.2"
  },
  "devDependencies": {
    "@babel/cli": "7.1.5",
    "@babel/core": "7.1.6",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/preset-env": "7.1.6",
    "@babel/preset-react": "7.0.0",
    "@epegzz/sass-vars-loader": "3.4.0",
    "@storybook/addon-info": "4.0.9",
    "@storybook/addon-knobs": "4.0.9",
    "@storybook/addon-options": "4.0.8",
    "@storybook/react": "4.0.9",
    "autoprefixer": "9.4.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "8.0.2",
    "babel-jest": "23.6.0",
    "babel-loader": "7.1.4",
    "babel-minify-webpack-plugin": "0.3.1",
    "babel-plugin-add-react-displayname": "0.0.5",
    "commitizen": "3.0.4",
    "cross-env": "5.2.0",
    "css-hot-loader": "1.4.3",
    "css-loader": "2.0.2",
    "cz-conventional-changelog": "2.1.0",
    "dotenv-webpack": "1.5.7",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.7.1",
    "eslint": "5.10.0",
    "eslint-config-standard": "12.0.0",
    "eslint-config-standard-react": "7.0.2",
    "eslint-import-resolver-webpack": "0.10.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-node": "8.0.0",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-react": "7.11.1",
    "eslint-plugin-standard": "4.0.0",
    "file-loader": "3.0.1",
    "html-webpack-plugin": "4.0.0-beta.5",
    "inquirer": "5.2.0",
    "jest": "23.6.0",
    "jest-fetch-mock": "2.1.0",
    "json-server": "0.14.0",
    "mini-css-extract-plugin": "0.5.0",
    "node-sass": "7.0.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "postcss-loader": "3.0.0",
    "react-hot-loader": "3.1.3",
